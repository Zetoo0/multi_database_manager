{
  "productName": "dbr",
  "version": "0.1.0",
  "identifier": "com.dbr.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "DBR",
        "label": "main",
        "width": 1280,
        "height": 1080,
        "minWidth": 640,
        "minHeight": 480,
        "maxHeight": 1080,
        "maxWidth": 1280,
        "titleBarStyle": "Overlay",
        "hiddenTitle": true,
        "resizable": true,
        "decorations": false,
        "dragDropEnabled": false
      }
    ],
    "security": {
      "csp": null,
      "assetProtocol": {
        "enable": true,
        "scope": ["**"]
      }
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
  
}
